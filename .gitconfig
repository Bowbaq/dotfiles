[user]
	name = Maxime Bury
	email = maxime.bury+github@gmail.com
[help]
	autocorrect = 1
[color]
	ui = true
[core]
	autocrlf = input
	excludesfile = /Users/bowbaq/.gitignore_global
	attributesfile = /Users/bowbaq/.gitattributes
[merge]
	tool = opendiff
	defaultToUpstream = true
[push]
	default = upstream
[mergetool]
	keepBackup = true
[credential]
	helper = osxkeychain
[diff "plist"]
	textconv = plutil -convert xml1 -o -
[alias]
	append = town append
	hack = town hack
	kill = town kill
	new-pull-request = town new-pull-request
	prepend = town prepend
	prune-branches = town prune-branches
	rename-branch = town rename-branch
	repo = town repo
	ship = town ship
	sync = town sync
